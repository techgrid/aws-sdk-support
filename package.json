{
  "name": "aws-sdk-support",
  "version": "1.0.0",
  "description": "Wrapper for AWS SDK to simplify calls",
  "main": "dist/aws-sdk-support.js",
  "types": "dist/",
  "scripts": {
    "test": "jest --coverage",
    "test:unit": "jest",
    "build": "webpack -p",
    "clean": "rm -rf -v dist/*"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/aws-lambda": "^8.10.3",
    "@types/chai": "^4.1.3",
    "@types/jest": "^23.3.14",
    "@types/node": "^10.14.4",
    "aws-sdk": "^2.566.0",
    "aws-sdk-mock": "^4.3.0",
    "jest": "^24.7.1",
    "jest-cli": "^25.1.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^4.2.0",
    "ts-mockito": "^2.3.1",
    "ts-node": "^6.0.3",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.12.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.8.3",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.7.0",
    "webpack-cli": "^3.3.0",
    "yaml-cfn": "^0.2.0",
    "zip-webpack-plugin": "^3.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
